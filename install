echo -e "\033[1;32mOpcion 12 \033[1;30mBorrar usuario o desconectarlo"
echo -e "\033[1;32mOpcion 11 \033[1;30mCambiar contrase√±a fecha o limite"
echo -e "\033[1;32mdropbear  \033[1;30mUsuarios conectados en Dropbear\033[0m"
echo -e "\033[1;32m@- \033[1;30mTelegram : @Pablo_Ezekiel\033[0m"
echo -e "\033[1;32mVpsPack  \033[1;30mInformacion General\033[0m"
echo -e "\033[1;32mInstalada \033[1;30mVersion 2.0 PLUS\033[0m"
